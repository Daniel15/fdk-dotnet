using FnProject.Fdk;
using System;

namespace {FUNCTION_NAME}
{
	class Program
	{
		static void Main(string[] args)
		{
			FdkHandler.Handle((ctx, input) =>
			{
				var inputStr = input.AsString();
				if (string.IsNullOrWhiteSpace(inputStr))
				{
					inputStr = "world";
				}
				return "Hello " + inputStr;
			});
		}
	}
}